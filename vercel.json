{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb"
            }
        },
        {
            "src": "agendaFront/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "agendaFront/$1"
        }
    ]
}